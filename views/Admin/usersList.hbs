
   

<table class="table" id="usersList">
  <thead>
    <tr>
     
      <th scope="col">First Name</th>
      <th scope="col">Last Name</th>
      <th scope="col">Email Address</th>
    </tr>
  </thead>
<tbody>
    {{#each processedUsers}}
    <tr>
      <td>{{this.firstName}}</td>
      <td>{{this.lastName}}</td>
      <td>{{email}}</td>
    </tr>
    {{/each}}
  </tbody>
</table>
<script>
  $(document).ready( function () {
    $('#usersList').DataTable();
} );
</script>
